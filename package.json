{
	"name": "jekyll-post",
	"displayName": "jekyll-post",
	"description": "Create new Jekyll post from template",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"author": {
		"name": "Rohan Garg",
		"email": "rhn.grg@gmail.com",
		"url": "rohgarg.github.io"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"bugs": {
		"url": ""
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.jekyllPost"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.jekyllPost",
				"title": "New Post"
			}
		],
		"menus": {
			"explorer/context": [{
				"command": "extension.jekyllPost",
				"group": "navigation@2"
			}]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
